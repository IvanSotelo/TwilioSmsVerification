{
    "name": "ivansotelo/twilio-verify",
    "description": "Verify Phone Numbers in a Laravel PHP Application with Twilio Verify",
    "keywords": [
        "laravel",
        "twilio",
        "verify",
        "sms"
    ],
    "type": "library",
    "scripts": {
        "test": "phpunit tests",
        "test:dist": "phpunit --coverage-clover clover.xml",
        "test:coverage": "php-coveralls -v -x clover.xml -o ./logs --exclude-no-stmt"
    },
    "require": {
        "php": ">=7.2",
        "illuminate/support": "^5.7|^6.0",
        "illuminate/config": "^5.7|^6.0"
    },
    "require-dev": {
        "laravel-notification-channels/twilio": "dev-master",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^4.0|^5.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "IvanSotelo\\TwilioVerify\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IvanSotelo\\TwilioVerify\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IvanSotelo\\TwilioVerify\\TwilioVerifyServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Sotelo",
            "email": "isotelo@controlla.com.mx"
        }
    ],
    "minimum-stability": "dev"
}
